<%
   container_id = 'bubble-chart-widget' + widget.id.to_s
   chartTitle = widget_properties["chartTitle"]
   chartHeight = widget_properties["chartHeight"]
   filterId = widget_properties["filter"].to_i
   xMetric = widget_properties["xMetric"]
